{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "description": "playground",
  "author": "Morgane Dubus <morgane.dubus@meilisearch.com>",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "restart": "kill-port --port 9000 && yarn build && yarn serve",
    "watch": "npm-watch restart"
  },
  "watch": {
    "restart": {
      "patterns": [
        "../gatsby-node.js",
        "../package.json",
        "../src",
        "./src",
        "./gatsby-config.js",
        "./package.json"
      ],
      "quiet": true
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@meilisearch/instant-meilisearch": "^0.6.0-beta.0",
    "gatsby": "^4.3.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-mdx": "^3.3.0",
    "gatsby-plugin-sharp": "^4.3.0",
    "gatsby-remark-images": "^6.3.0",
    "gatsby-source-filesystem": "^4.3.0",
    "instantsearch.css": "^7.4.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-instantsearch-dom": "^6.16.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "kill-port": "^1.6.1",
    "npm-watch": "^0.11.0"
  }
}
